auto eth0
iface eth0 inet static
  address 192.168.1.25
  netmask 255.255.255.0
  gateway 192.168.1.1
  #dns-nameservers 192.168.1.1

auto eth1
iface eth1 inet static
   address 10.8.5.25
   netmask 255.255.254.0
   gateway 10.8.4.1
#   address 172.16.141.171
#   gateway 172.16.141.1
#   netmask 255.255.254.0
   post-up ip link add link eth1 name vrsu    type vlan id 3
   post-up ip link add link eth1 name vcan    type vlan id 4


auto vrsu
iface vrsu inet static
   address 192.168.3.25
   netmask 255.255.255.0

auto vcan
iface vcan inet static
   address 192.168.4.25
   netmask 255.255.255.0

